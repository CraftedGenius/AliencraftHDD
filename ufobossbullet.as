package  {		import flash.display.MovieClip;	import flash.events.Event;			public class ufobossbullet extends MovieClip {						public function ufobossbullet() {			// constructor code			addEventListener(Event.ENTER_FRAME, pulse);		}				public function stopListening()		{			removeEventListener(Event.ENTER_FRAME, pulse);		}				public function startListening()		{			addEventListener(Event.ENTER_FRAME, pulse);		}				public function pulse (evt:Event)    	{		    if(currentFrame > 30) return;			this.x = this.x + 15;		}	}	}