package  {		import flash.display.MovieClip;	import flash.events.Event;	import flash.media.Sound; 	import flash.net.URLRequest; 		public class forestufoboss extends MovieClip {				//variables		var yMovement:Number = 5;		var changeDirectionAfterYMoves:Number = 0;		var moveYCount:Number = 0;				var shootCount:Number = 0;		var shootSpecialCount:Number = 0;		var shootUfoBulletAfterXMoves:Number = 30;		var shootSpecialUfoBulletAfterXMoves:Number = 300;				public function forestufoboss() {			// constructor code			addEventListener(Event.ENTER_FRAME, pulse);		}		//stops the ufoboss stuff carrying on when game ends		public function stopListening()		{			removeEventListener(Event.ENTER_FRAME, pulse);		}				public function startListening()		{			addEventListener(Event.ENTER_FRAME, pulse);		}				public function pulse(evt:Event)		{			if(currentFrame>35) return;						if (changeDirectionAfterYMoves == moveYCount)			{				yMovement = yMovement * -1;			}							var maxYMoves:Number;				if(yMovement > 0)					{						maxYMoves = Math.round((400-this.y)/yMovement);					}				else 					{						maxYMoves = Math.round((this.y-50)/Math.abs(yMovement));					}								changeDirectionAfterYMoves = 1 + Math.round(Math.random()*maxYMoves)				moveYCount = 0;									if (shootCount > shootUfoBulletAfterXMoves)			{				var snd:Sound = new Sound(new URLRequest("2ndbossnormbullet.mp3")); 				snd.play();				var m:forestufobossbullet = new forestufobossbullet();				m.x = this.x;				m.y = this.y;				parent.addChild(m);				shootCount = 0;				shootUfoBulletAfterXMoves = 1 + Math.round(Math.random()*30);			}			if (shootSpecialCount > shootSpecialUfoBulletAfterXMoves)			{				var specialsnd:Sound = new Sound(new URLRequest("2ndbossspecialbullet.mp3")); 				specialsnd.play();				var n:forestufobossspecialbullet = new forestufobossspecialbullet();				n.x = this.x;				n.y = this.y;				parent.addChild(n);				shootSpecialCount = 0;				shootSpecialUfoBulletAfterXMoves = 1 + Math.round(Math.random()*300);			}			this.y += yMovement;			moveYCount = moveYCount + 1;			shootCount = shootCount + 1;			shootSpecialCount = shootSpecialCount + 1;		}	}	}