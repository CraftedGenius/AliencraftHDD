package  {		import flash.display.MovieClip;	import flash.events.Event;	import flash.media.Sound; 	import flash.net.URLRequest; 		public class forestufo extends MovieClip {				//variables		var xMovement:Number = 5;		var yMovement:Number = 5;		var changeDirectionAfterXMoves:Number = 0;		var changeDirectionAfterYMoves:Number = 0;		var moveCount:Number = 0;		var moveYCount:Number = 0;        var maxYMoves:Number;				var shootCount:Number = 0;		var shootUfoBulletAfterXMoves:Number = 40;				public function forestufo() {			// constructor code			addEventListener(Event.ENTER_FRAME, pulse);		}				//stops the ufo stuff carrying on when game ends		public function stopListening()		{			removeEventListener(Event.ENTER_FRAME, pulse);		}				public function startListening()		{			addEventListener(Event.ENTER_FRAME, pulse);		}				public function pulse(evt:Event)		{			if(currentFrame>30) return;			if (changeDirectionAfterXMoves == moveCount)			{				xMovement = xMovement * -1;				var maxMoves:Number;				if(xMovement > 0)				{					maxMoves = Math.round((300-this.x)/xMovement);				}				else 				{					maxMoves = Math.round((this.x-50)/Math.abs(xMovement));				}				changeDirectionAfterXMoves = 1 + Math.round(Math.random()*maxMoves)				moveCount = 0;			}						if (changeDirectionAfterYMoves == moveYCount)			{				yMovement = yMovement * -1;			}											if(yMovement > 0)					{						maxYMoves = Math.round((450-this.y)/yMovement);					}				else 					{						maxYMoves = Math.round((this.y-50)/Math.abs(yMovement));					}								changeDirectionAfterYMoves = 1 + Math.round(Math.random()*maxYMoves)				moveYCount = 0;									if (shootCount > shootUfoBulletAfterXMoves)			{				var snd:Sound = new Sound(new URLRequest("m16.mp3")); 				snd.play();				var m:forestufobullet = new forestufobullet();				m.x = this.x;				m.y = this.y;				var n:forestufobullet = new forestufobullet();				n.x = this.x - 20;				n.y = this.y;				var o:forestufobullet = new forestufobullet();				o.x = this.x - 40;				o.y = this.y;								parent.addChild(m);				parent.addChild(n);				parent.addChild(o);				shootCount = 0;				shootUfoBulletAfterXMoves = 30 + Math.round(Math.random()*110);			}			this.x += xMovement;			this.y += yMovement;			moveCount = moveCount + 1;			moveYCount = moveYCount + 1;			shootCount = shootCount + 1;		}			}	}