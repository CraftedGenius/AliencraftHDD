package  {		import flash.display.MovieClip;	import flash.events.Event;			public class caveufobossbullet extends MovieClip {				var yMovement:Number = 10;				public function caveufobossbullet() {			// constructor code			addEventListener(Event.ENTER_FRAME, pulse);		}				public function stopListening()		{			removeEventListener(Event.ENTER_FRAME, pulse);		}				public function startListening()		{			addEventListener(Event.ENTER_FRAME, pulse);		}				public function pulse (evt:Event)    	{		    if(currentFrame > 30) return;			this.y = this.y + yMovement;			this.x = this.x + 10;				if(this.y>480-this.height)//off bottom edge			{				yMovement = yMovement * -1;			}				if(this.y<0+this.height) //off top edge			{				yMovement = yMovement * -1;			}		}	}	}